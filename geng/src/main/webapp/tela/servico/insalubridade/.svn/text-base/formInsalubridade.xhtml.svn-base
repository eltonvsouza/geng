<?xml version="1.0" encoding="ISO-8859-1"?>
<html	
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
<f:loadBundle basename="message" var="inter" />
<rich:jQuery timing="onload" selector="#porcentagem" query="focus()" />
<h:panelGrid columns="2">
   	<h:outputText value="*#{inter.servico}" style="float: right;"/>
	<h:selectOneMenu  value="#{insalubridadeBean.insalubridade.servico}" id="servico">
		<f:selectItems value="#{servicoBean.listaServico}" />
	</h:selectOneMenu>
   	
	<h:outputText value="*#{inter.porcentagem}" style="float: right;"/>
	<h:inputText value="#{insalubridadeBean.insalubridade.porcentagem}" maxlength="3" style=" width:25px; align:left;" id="porcentagem"
																	    converterMessage="Campo Código: Apenas números são aceitos">
		<f:convertNumber pattern="###" integerOnly="true"/>
	</h:inputText>
</h:panelGrid>
</html>